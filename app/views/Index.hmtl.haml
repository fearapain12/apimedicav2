h1.title All Users

%table#table
  %thead
    %tr
      %th ID
      %th Username
      -#%th
      -#%th
  %tbody
    - @users.each do |user|
      %tr
        %td= link_to user.id, user
        %td= link_to user.username, user
        -#%td= link_to 'Edit', edit_user_path(user)
        -#%td= link_to 'Delete', user_path(user), :method => :delete, data: { confirm: 'Are you sure?' }

%p=link_to 'Create new user', new_user_path